<!DOCTYPE HTML>
<html>  <head>
    <meta charset="UTF-8">
    <link href="https://cdn.jsdelivr.net/npm/halfmoon@1.1.1/css/halfmoon-variables.min.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/halfmoon@1.1.1/js/halfmoon.min.js"></script>
    <style>
      ul ul {margin: 10px 0 15px 20px}
      li {margin-bottom: 10px;}
    </style>
  </head>
  <body>
    <div class="page-wrapper with-sidebar with-navbar">
      <nav class="navbar">
        <a href="index.html" class="navbar-brand">{{config.title}}</a>
        <div class="navbar-content ml-auto">
          <button class="btn" type="button">Graph</button>
        </div>
        &nbsp;
        <div class="dropdown">
          <button class="btn" data-toggle="dropdown" type="button" id="dropdown-toggle-btn-1" aria-haspopup="true" aria-expanded="false">
            <svg width="1.1em" height="1.1em" viewBox="-4 -8 90 80" focusable="false" aria-hidden="true" role="img">
              <path fill="currentColor" d="M61 24.6h7.9l18.7 51.6h-7.7l-5.4-15.5H54.3l-5.6 15.5h-7.2L61 24.6zM72.6 55l-8-22.8L56.3 55h16.3z"></path>
              <path fill="currentColor" d="M53.6 60.6c-10-4-16-9-22-14 0 0 1.3 1.3 0 0-6 5-20 13-20 13l-4-6c8-5 10-6 19-13-2.1-1.9-12-13-13-19h8c4 9 10 14 10 14 10-8 10-19 10-19h8s-1 13-12 24c5 5 10 9 19 13l-3 7zm-52-44h56v-8h-23v-7h-9v7h-24v8z"></path>
            </svg>
            &nbsp;{{default_lang}}
          </button>
          <div class="dropdown-menu" aria-labelledby="dropdown-toggle-btn-1">
            {% for language in languages %}
            <a href="../{{language.path}}/index.html" class="dropdown-item">{{language.title}}</a>
            {% endfor %}
          </div>
        </div>
      </nav>
      <div class="sidebar" style="margin-left:10px">
        <div class="sidebar-menu font-size-16" style="margin-left:10px">
          {{summary}}
        </div>
      </div>
      <div class="content-wrapper" style="margin-left:50px">
      </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/cash/8.1.2/cash.min.js"></script>
    <script src="tree.js"></script>
    <script>
      window.onload=function(){
        set_tree(".sidebar-menu");
      }
    </script>
  </body>
</html>